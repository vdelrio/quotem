{
  "expo": {
    "name": "QuoteM",
    "slug": "quotem",
    "scheme": "quotem",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/q-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vdelrio.quotem"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/q-adaptive-icon.png",
        "backgroundColor": "#ff9800"
      },
      "package": "com.vdelrio.quotem"
    },
    "web": {
      "favicon": "./assets/q-favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/pt-sans-narrow/PTSansNarrow_400Regular.ttf"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/q-adaptive-icon.png",
          "imageWidth": 200
        }
      ]
    ]
  }
}
