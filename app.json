{
  "expo": {
//    "name": "QuoteM",
    "name": "QuoteM (DEBUG)",
    "slug": "quotem",
    "scheme": "quotem",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/q-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vdelrio.quotem"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/q-adaptive-icon.png",
        "backgroundColor": "#ec6a52"
      },
//      "package": "com.vdelrio.quotem"
      "package": "com.vdelrio.quotem.debug"
    },
    "web": {
      "favicon": "./assets/q-favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/pt-sans-narrow/PTSansNarrow_400Regular.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ec6a52",
          "image": "./assets/q-adaptive-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) necesita acceso a tu c√°mara."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5b950766-7821-4013-a990-c4afc92973a9"
      }
    },
    "owner": "vdelrio"
  }
}
